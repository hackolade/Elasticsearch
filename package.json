{
    "name": "Elasticsearch",
    "version": "00.2.0",
    "versionDate": "2023-07-20",
    "author": "hackolade",
    "engines": {
        "hackolade": "1.12.7",
        "hackoladePlugin": "1.0.0"
    },
    "contributes": {
        "target": {
            "applicationTarget": "ELASTICSEARCH",
            "title": "Elasticsearch",
            "versions": [
                "2.x",
                "5.x",
                "6.x"
            ]
        },
        "features": {
            "disableMultipleTypes": true,
            "enableReverseEngineering": true,
            "enableForwardEngineering": true,
            "enableReverseEngineeringDetectionSchemaByContent": true
        }
    },
    "description": "Hackolade plugin for Elasticsearch 2.0 thru 6.8.  For version 7.0 and above requires a new plugin!",
    "dependencies": {
        "async": "2.5.0",
        "elasticsearch": "13.3.1",
        "lodash": "4.17.5"
    },
    "lint-staged": {
        "*.{js,json}": "prettier --write"
    },
    "simple-git-hooks": {
        "pre-commit": "npx lint-staged",
        "pre-push": "npx eslint ."
    },
    "scripts": {
        "lint": "eslint . --max-warnings=0",
        "package": "node esbuild.package.js"
    },
    "devDependencies": {
        "@hackolade/hck-esbuild-plugins-pack": "0.0.1",
        "esbuild": "0.21.4",
        "esbuild-plugin-clean": "1.0.1",
        "eslint": "9.3.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-prettier": "5.1.3",
        "lint-staged": "14.0.1",
        "prettier": "3.2.5",
        "simple-git-hooks": "2.11.1"
    }
}
